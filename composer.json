{
    "name": "aoe/module-skeleton",
    "description": "This Module provides a basic Skeleton for further Magento 2 Modules.",
    "license": "GPL-3.0",
    "type": "magento2-module",
    "homepage": "http://aoe.com/",
    "keywords":[ "composer-installer", "magento", "Magento 2 Module", "Skeleton", "Magento 2", "AOE"],
    "authors": [
        {
            "email": "david.lambauer@aoe.com",
            "name": "David Lambauer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "magento/framework": "100.*",
        "php": ">=7.0.14"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6"
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Aoe\\Skeleton\\": ""
        }
    }
}
